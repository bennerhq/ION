int fill_grid(int[][] grid, int base)
    int i = 0
    int v = base
    while i < grid.length()
        int j = 0
        while j < grid[i].length()
            grid[i][j] = v
            v = v + 1
            j = j + 1
        i = i + 1
    return v

int sum_grid(int[][] grid)
    int total = 0
    int i = 0
    while i < grid.length()
        int j = 0
        while j < grid[i].length()
            total = total + grid[i][j]
            j = j + 1
        i = i + 1
    return total

void main()
    int[][] grid = new int[][][3]
    int i = 0
    while i < 3
        grid[i] = new int[6]
        i = i + 1
    int endv = fill_grid(grid, 5)
    int sum = sum_grid(grid)
    print("%i %i\n", endv, sum)
