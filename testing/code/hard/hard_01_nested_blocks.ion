void main()
    int i = 0
    int sum = 0
    while i < 3
        if i == 1
            int j = 0
            while j < 2
                if j == 1
                    sum = sum + 20
                else
                    sum = sum + 1
                j = j + 1
        else
            sum = sum + 10
        i = i + 1
    print(sum)
