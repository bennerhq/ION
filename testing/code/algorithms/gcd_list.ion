int gcd(int a, int b)
    while b != 0
        int t = a % b
        a = b
        b = t
    return a

void main()
    int[] arr = new int[3]
    arr[0] = 48
    arr[1] = 18
    arr[2] = 30
    int g = arr[0]
    int i = 1
    while i < arr.length()
        g = gcd(g, arr[i])
        i = i + 1
    print(g)
