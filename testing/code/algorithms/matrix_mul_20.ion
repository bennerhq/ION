matrix:
    int n
    int[] data

int index(matrix m, int i, int j)
    return i * m.n + j

void set(matrix m, int i, int j, int v)
    m.data[index(m, i, j)] = v

int get(matrix m, int i, int j)
    return m.data[index(m, i, j)]

matrix new_matrix(int n)
    matrix m = new matrix
    m.n = n
    m.data = new int[n * n]
    return m

void main()
    int n = 20
    matrix a = new_matrix(n)
    matrix b = new_matrix(n)
    matrix c = new_matrix(n)
    int i = 0
    int j = 0
    int k = 0
    int sum = 0
    int total = 0
    while i < n
        j = 0
        while j < n
            set(a, i, j, i + j)
            set(b, i, j, i - j)
            j = j + 1
        i = i + 1
    i = 0
    while i < n
        j = 0
        while j < n
            k = 0
            sum = 0
            while k < n
                sum = sum + get(a, i, k) * get(b, k, j)
                k = k + 1
            set(c, i, j, sum)
            j = j + 1
        i = i + 1
    i = 0
    while i < n
        j = 0
        while j < n
            total = total + get(c, i, j)
            j = j + 1
        i = i + 1
    print("%i\n", total)
