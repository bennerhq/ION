void bubble_sort(int n)
    int[] arr = new int[n]
    int i = 0
    while i < n
        arr[i] = n - i
        i = i + 1

    i = 0
    while i < n
        int j = 0
        while j < n - 1
            if arr[j] > arr[j + 1]
                int tmp = arr[j]
                arr[j] = arr[j + 1]
                arr[j + 1] = tmp
            j = j + 1
        i = i + 1

    print("%i\n", arr[0])

void main()
    bubble_sort(100000)
