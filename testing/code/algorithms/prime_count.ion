bool is_prime(int n)
    if n < 2
        return false
    int i = 2
    while i * i <= n
        if n % i == 0
            return false
        i = i + 1
    return true

void main()
    int limit = 10000000
    int count = 0
    int i = 0
    while i <= limit
        if is_prime(i)
            count = count + 1
        i = i + 1
    print("%i\n", count)
