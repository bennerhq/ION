void main()
    int[] arr = new int[5]
    arr[0] = 5
    arr[1] = 1
    arr[2] = 4
    arr[3] = 2
    arr[4] = 3
    int n = 5
    int i = 1
    while i < n
        int key = arr[i]
        int j = i - 1
        int pos = i
        while j >= 0
            if arr[j] > key
                arr[j + 1] = arr[j]
                pos = j
                j = j - 1
            else
                j = -1
        arr[pos] = key
        i = i + 1
    i = 0
    while i < n
        print("%i ", arr[i])
        i = i + 1
    print("\n")
