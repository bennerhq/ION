bool is_prime(int n)
    if n < 2
        return false
    int i = 2
    while i * i <= n
        if n % i == 0
            return false
        i = i + 1
    return true

void main()
    int n = 2
    int sum = 0
    while n <= 50
        if is_prime(n)
            sum = sum + n
        n = n + 1
    print(sum)
