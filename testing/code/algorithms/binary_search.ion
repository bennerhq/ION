int binary_search(int[] xs, int target)
    int lo = 0
    int hi = xs.length() - 1
    while lo <= hi
        int mid = (lo + hi) / 2
        if xs[mid] == target
            return mid
        if xs[mid] < target
            lo = mid + 1
        else
            hi = mid - 1
    return -1

void main()
    int[] xs = new int[7]
    xs[0] = 1
    xs[1] = 3
    xs[2] = 4
    xs[3] = 6
    xs[4] = 8
    xs[5] = 9
    xs[6] = 11
    print("%i\n", binary_search(xs, 6))
    print("%i\n", binary_search(xs, 2))
