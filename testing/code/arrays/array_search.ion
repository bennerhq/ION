int find_index(int[] xs, int target)
    int i = 0
    while i < xs.length()
        if xs[i] == target
            return i
        i = i + 1
    return -1

void main()
    int[] xs = new int[6]
    xs[0] = 9
    xs[1] = 4
    xs[2] = 7
    xs[3] = 1
    xs[4] = 8
    xs[5] = 2
    print("%i\n", find_index(xs, 7))
    print("%i\n", find_index(xs, 5))
