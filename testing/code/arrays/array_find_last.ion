int find_last(int[] arr, int target)
    int i = arr.length() - 1
    while i >= 0
        if arr[i] == target
            return i
        i = i - 1
    return -1

void main()
    int[] arr = new int[5]
    arr[0] = 1
    arr[1] = 2
    arr[2] = 3
    arr[3] = 2
    arr[4] = 4
    print(find_last(arr, 2))
