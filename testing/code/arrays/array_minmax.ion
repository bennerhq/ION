void main()
    int[] arr = new int[5]
    arr[0] = 3
    arr[1] = 7
    arr[2] = 2
    arr[3] = 9
    arr[4] = 5

    int min = arr[0]
    int max = arr[0]
    int i = 1
    while i < 5
        if arr[i] < min
            min = arr[i]
        if arr[i] > max
            max = arr[i]
        i = i + 1
    print("%i %i\n", min, max)
