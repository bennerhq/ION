int fill_sum(int[] arr, int start, int step)
    int i = 0
    int v = start
    int sum = 0
    while i < arr.length()
        arr[i] = v
        if v % 3 == 0
            sum = sum + v
        else
            sum = sum + (v % 5)
        v = v + step
        i = i + 1
    return sum

int prefix_end(int[] arr)
    int i = 1
    while i < arr.length()
        arr[i] = arr[i] + arr[i - 1]
        i = i + 1
    return arr[arr.length() - 1]

void main()
    int[] arr = new int[5]
    int s = fill_sum(arr, 3, 3)
    int p = prefix_end(arr)
    print("%i %i\n", s, p)
